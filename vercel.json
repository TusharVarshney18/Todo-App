{
   "version": 2,
   "builds": [
      {
         "src": "index.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "index.js"
      }
   ],
   "headers": [
      {
         "source": "/(.*)",
         "headers": [
            {
               "key": "Content-Security-Policy",
               "value": "default-src 'self'; script-src 'self' https://vercel.live; connect-src 'self' https://todo-app-frontend-x8wj.vercel.app;"
            },
            {
               "key": "Access-Control-Allow-Origin",
               "value": "https://todo-app-frontend-x8wj.vercel.app"
            },
            {
               "key": "Access-Control-Allow-Credentials",
               "value": "true"
            },
            {
               "key": "Access-Control-Allow-Methods",
               "value": "GET,POST,PUT,DELETE,OPTIONS"
            },
            {
               "key": "Access-Control-Allow-Headers",
               "value": "Content-Type, Authorization"
            }
         ]
      }
   ]
}